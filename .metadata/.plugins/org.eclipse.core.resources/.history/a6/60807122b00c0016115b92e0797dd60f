#include "stdbool.h"

#ifndef CIRBUFFER_H_
#define CIRBUFFER_H_

#define TRUE 1;
#define FALSE 0;


#endif /* CIRBUFFER_H_ */


typedef struct cBuffer
{
	int index;
	int begin;
	int end;
	int size;
	int* data;
} circBuffer;

circBuffer* cbuff_init(int bufferSize);
_Bool cbuff_isEmpty(circBuffer* self);
void cbuff_push(circBuffer* self, int input);
